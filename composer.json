{
    "name": "slub/mpdb-web",
    "type": "typo3-cms-extension",
    "description": "templates for music publisher database",
    "homepage": "https://musikverlage.slub-dresden.de",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS"],
    "require": {
        "typo3/cms-core": "^11",
        "bk2k/bootstrap-package": "^12",
        "georgringer/news": "^9",
        "slub/slub-be-users": "^11.5",
        "typo3/cms-rte-ckeditor": "^11",
        "typo3/cms-fluid-styled-content": "^11",
        "causal/ig_ldap_sso_auth": "^3",
        "slub/mpdb-presentation" : "@dev"
    },
    "autoload": {
        "psr-4": {
            "Slub\\MpdbWeb\\": "Classes/"
